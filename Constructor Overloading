#include <iostream>
using namespace std;

class Rectangle {
    int length, width;
public:
    // Default constructor
    Rectangle() {
        length = 0;
        width = 0;
    }

    // Parameterized constructor
    Rectangle(int l, int w) {
        length = l;
        width = w;
    }

    void display() {
        cout << "Length: " << length << ", Width: " << width << endl;
    }
};

int main() {
    Rectangle r1;        // Calls default constructor
    Rectangle r2(10, 5); // Calls parameterized constructor

    r1.display();
    r2.display();
    return 0;
}
