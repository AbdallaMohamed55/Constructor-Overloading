#include <iostream>
using namespace std;

class Student {
    string name;
    int age;
public:
    Student(string n, int a) {
        name = n;
        age = a;
        cout << "Name: " << name << ", Age: " << age << endl;
    }
};

int main() {
    Student s1("Rahul", 20);
    Student s2("Anita", 22);
    return 0;
}
